{
    "manifest_version": 2,
    "name": "Auto Catizen",
    "version": "1.0",
    "description": "Auto Catizen",
    "permissions": [
        "<all_urls>"
    ],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "logo.png",
        "48": "logo.png",
        "128": "logo.png"
      }
    },
    "icons": {
      "16": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
    },
    "content_scripts": [
      {
        "matches": ["*://*.catizen.ai/*"],
        "js": ["content.js"],
        "all_frames": true
      }
    ]
  }